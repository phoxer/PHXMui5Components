{
  "name": "@phoxer/react-components",
  "version": "1.0.5",
  "description": "Phoxer React Components",
  "scripts": {
    "rollup": "rollup -c",
    "build": "rm -rf dist/ && prettier -- write src/ && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs"
  },
  "keywords": [
    "Phoxer",
    "React",
    "Components"
  ],
  "author": "Phoxer",
  "license": "MIT",
  "devDependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/material": "^5.10.7",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^14.1.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@types/date-fns": "^2.6.0",
    "@types/react": "^18.0.21",
    "date-fns": "^2.29.3",
    "react": "^18.2.0",
    "rollup": "^2.79.1",
    "rollup-plugin-dts": "^4.2.2",
    "tslib": "^2.4.0",
    "typescript": "^4.8.3",
    "@types/node": "^18.7.23"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/phoxer"
  },
  "types": "dist/index.d.ts"
